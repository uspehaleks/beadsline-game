{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/dist/public/api/index.cjs"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/dist/public/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/public/index.html"
    }
  ]
}