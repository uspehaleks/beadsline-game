{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/public/api/index.cjs"
    },
    {
      "src": "/assets/(.*)",
      "dest": "dist/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/public/index.html"
    }
  ]
}
